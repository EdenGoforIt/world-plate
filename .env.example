# API Configuration
API_BASE_URL=https://api.worldplate.com
API_VERSION=v1
API_TIMEOUT=30000

# Authentication
AUTH_DOMAIN=auth.worldplate.com
AUTH_CLIENT_ID=your_client_id_here
AUTH_REDIRECT_URI=worldplate://auth

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PUSH_NOTIFICATIONS=true

# Third Party Services
SENTRY_DSN=your_sentry_dsn_here
GOOGLE_MAPS_API_KEY=your_google_maps_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_key_here

# Development
DEBUG_MODE=false
LOG_LEVEL=error
MOCK_API=false